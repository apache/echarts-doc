    "chalk": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-dir": "^0.3.0",
    "css-loader": "^3.5.3",
    "etpl": "~3.0.1",
    "file-loader": "^4.3.0",
    "fs-extra": "^8.1.0",
    "glob": "^6.0.3",
    "htmlparser2": "^4.1.0",
    "install": "^0.13.0",
    "jsdom": "^9.8.3",
    "marked": "^0.3.19",
    "mini-css-extract-plugin": "^0.8.2",
    "node-static": "^0.7.11",
    "npm": "^6.14.5",
    "open": "^7.0.4",
    "sass.js": "^0.11.1",
